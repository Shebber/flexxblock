{
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/data/(.*)", "dest": "/public/data/$1" },
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/style\\.css", "dest": "/public/style.css" },
    { "src": "/script\\.js", "dest": "/public/script.js" },
    { "src": "/.*", "dest": "/public/index.html" }
  ]
}
